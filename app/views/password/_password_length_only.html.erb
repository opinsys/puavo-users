<script>
function validatePassword_LengthOnly(password)
{
  if (password.length < <%= min_length %>)
    return PASSWORD_TOO_SHORT;

  return PASSWORD_OK;
}

const STRINGS = {
<% strings.each do |k, v| %>
  "<%= k %>": "<%= v %>",
<% end %>
};

initPasswordValidator("<%= password_id %>",
                      "<%= confirm_id %>",
                      validatePassword_LengthOnly,
                      <%= min_length %>,
                      STRINGS);
</script>
